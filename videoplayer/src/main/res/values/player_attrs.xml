<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PlayerView">
        <attr name="playmode">
            <enum name="fullscreen" value="0" />
            <enum name="window" value="1" />
            <enum name="miniwindow" value="2" />
        </attr>
        <attr name="lockmode" format="boolean"></attr>
        <attr name="layout_height" format="integer"></attr>
        <attr name="layout_width" format="integer"></attr>
    </declare-styleable>

</resources>